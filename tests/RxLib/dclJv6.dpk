{$I JEDI.INC}

package DclJv6;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS OFF}
{$BOOLEVAL OFF}
{$DEBUGINFO OFF}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS OFF}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO OFF}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST ON}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'Jv Library Controls and Tools'}
{$DESIGNONLY}
{$IMPLICITBUILD ON}

requires
  JvCtl6,
  dclsmp,
  dclstd,
  designide;

contains
  JvLConst in 'JvLConst.pas',
  JvHintProp in 'JvHintProp.pas',
  JvCheckItm in 'JvCheckItm.pas',
  JvStrLEdit in 'JvStrLEdit.pas',
  JvColors in 'JvColors.pas',
  JvDsgn in 'JvDsgn.pas',
  JvPresrDsn in 'JvPresrDsn.pas',
  JvMinMaxEd in 'JvMinMaxEd.pas',
  JvSbEdit in 'JvSbEdit.pas',
  JvPgMngrEd in 'JvPgMngrEd.pas',
  JvTimLstEd in 'JvTimLstEd.pas',
  JvIcoLEdit in 'JvIcoLEdit.pas',
  JvResExp in 'JvResExp.pas',
  JvPictEdit in 'JvPictEdit.pas',
  JvDirFrm in 'JvDirFrm.pas',
  JvGradEdit in 'JvGradEdit.pas',
  JvCtlReg in 'JvCtlReg.pas',
  JvTooReg in 'JvTooReg.pas';

end.
