{$I JEDI.INC}

package JvxBDE6;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS OFF}
{$BOOLEVAL OFF}
{$DEBUGINFO OFF}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS OFF}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO OFF}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST ON}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'Jvx Library BDE DB Component Package'}
{$RUNONLY}
{$IMPLICITBUILD ON}

requires
  vcldb,
  bdertl,
  dsnap,
  JvxCtl6,
  JvxDb6;

contains
  JvxDBQBE in 'JvxDBQBE.pas',
  JvxDBFilter in 'JvxDBFilter.pas',
  JvxDBLists in 'JvxDBLists.pas',
  JvxDBIndex in 'JvxDBIndex.pas',
  JvxDBPrgrss in 'JvxDBPrgrss.pas',
  JvxLoginDlg in 'JvxLoginDlg.pas',
  JvxDBSecur in 'JvxDBSecur.pas',
  JvxRemLog in 'JvxRemLog.pas',
  JvxChPswDlg in 'JvxChPswDlg.pas',
  JvxBdeUtils in 'JvxBdeUtils.pas',
  JvxQuery in 'JvxQuery.pas',
  JvxMemTable in 'JvxMemTable.pas',
  JvxDBExcpt in 'JvxDBExcpt.pas';

end.
