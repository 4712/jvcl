  JvQSoundControl in '..\..\qrun\JvQSoundControl.pas',
  JvQSystemColors in '..\..\qrun\JvQSystemColors.pas',
  JvQThread in '..\..\qrun\JvQThread.pas',
  JvQThreadTimer in '..\..\qrun\JvQThreadTimer.pas',
  JvQTimer in '..\..\qrun\JvQTimer.pas',
  JvQAppXmlStorage in '..\..\qrun\JvQAppXMLStorage.pas';

